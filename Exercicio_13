#include <iostream>
# include <cmath>

using namespace std;

int main()
{

   float m1, m2, m3, r12, r13, r23, ee, g;

      cout << "Informe as tres massas: ";
      cin >> m1;
      cin >> m2;
      cin >> m3;
      cout << "Informe as tres distancias: ";
      cin >> r12;
      cin >> r13;
      cin >> r23;

      g = 6.67*(pow(10,-11));

      ee = g*(((m1*m2)/r12)+((m1*m3)/r13)+((m2*m3)/r23));

      cout << "Valor de coesao: " <<ee;

    return 0;
}